        <rdfs:isDefinedBy rdf:resource="http://rdfs.co/bevon/"/>
    </owl:Class>
    


    <!-- http://rdfs.co/bevon/Longneck -->

    <owl:Class rdf:about="&bevon;Longneck">
        <rdfs:subClassOf rdf:resource="&bevon;BeerBottle"/>
        <dcterms:issued rdf:datatype="&xsd;date">2013-07-04+09:00</dcterms:issued>
        <dcterms:modified rdf:datatype="&xsd;date">2014-03-01+09:00</dcterms:modified>
        <skos:definition xml:lang="en">
    A longneck is a type of beer bottle with a long neck.
    </skos:definition>
        <rdfs:comment xml:lang="en">
    It is known as the standard longneck bottle or industry standard bottle
    (ISB). The ISB longnecks have a uniform capacity, height, weight and
    diameter and can be reused on average 16 times. The long neck offers a long
    cushion of air to absorb the pressure of carbonation to reduce the risk of
    exploding. The longneck also provides a handle for drinking directly from
    the bottle without transferring body heat to the beer from one&apos;s hand.
    </rdfs:comment>
        <skos:altLabel xml:lang="en">Industry Standard Bottle</skos:altLabel>
        <skos:prefLabel xml:lang="en">Longneck</skos:prefLabel>
        <skos:altLabel xml:lang="en">North American Longneck</skos:altLabel>
        <status:term_status xml:lang="en">unstable</status:term_status>
        <skos:prefLabel xml:lang="ko">긴목 맥주병</skos:prefLabel>
        <rdfs:isDefinedBy rdf:resource="http://rdfs.co/bevon/"/>
    </owl:Class>
    


