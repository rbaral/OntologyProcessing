    <owl:Class rdf:about="&bevon;Container">
        <rdfs:subClassOf rdf:resource="&yago;Containers"/>
        <dcterms:issued rdf:datatype="&xsd;date">2013-07-04+09:00</dcterms:issued>
        <dcterms:modified rdf:datatype="&xsd;date">2014-03-01+09:00</dcterms:modified>
        <skos:definition xml:lang="en">
    A container is an item used to contain, store, and transport a beverage.
    </skos:definition>
        <skos:prefLabel xml:lang="en">Container</skos:prefLabel>
        <status:term_status xml:lang="en">unstable</status:term_status>
        <skos:prefLabel xml:lang="ko">용기</skos:prefLabel>
        <rdfs:seeAlso rdf:resource="http://en.wikipedia.org/wiki/Container"/>
        <rdfs:isDefinedBy rdf:resource="http://rdfs.co/bevon/"/>
    </owl:Class>
    


    <!-- http://rdfs.co/bevon/DilutedSoju -->

    <owl:Class rdf:about="&bevon;DilutedSoju">
        <rdfs:subClassOf rdf:resource="&bevon;Soju"/>
        <owl:disjointWith rdf:resource="&bevon;DistilledSoju"/>
        <dcterms:issued rdf:datatype="&xsd;date">2013-07-04+09:00</dcterms:issued>
        <dcterms:modified rdf:datatype="&xsd;date">2014-03-01+09:00</dcterms:modified>
        <skos:definition xml:lang="en">
    Diluted Soju is made by mixing pure ethanol distilled from various grains,
    such as potatoes, wheat, barley, sweet potatoes, or tapioca, with water and
    flavoring.
    </skos:definition>
        <skos:prefLabel xml:lang="en">Diluted Soju</skos:prefLabel>
        <status:term_status xml:lang="en">unstable</status:term_status>
        <skos:prefLabel xml:lang="ko">희석식 소주</skos:prefLabel>
        <rdfs:isDefinedBy rdf:resource="http://rdfs.co/bevon/"/>
    </owl:Class>
    


    <!-- http://rdfs.co/bevon/DistilledBeverage -->

    <owl:Class rdf:about="&bevon;DistilledBeverage">
        <rdfs:subClassOf rdf:resource="&bevon;AlcoholicBeverage"/>
        <rdfs:subClassOf rdf:resource="&pto;Distilled_beverage"/>
        <dcterms:issued rdf:datatype="&xsd;date">2013-07-04+09:00</dcterms:issued>
        <dcterms:modified rdf:datatype="&xsd;date">2014-03-01+09:00</dcterms:modified>
        <skos:definition xml:lang="en">
     A distilled beverage, spirit, or liquor is an alcoholic beverage
     containing ethanol that is produced by distilling (i.e., concentrating
     by distillation) ethanol produced by means of fermenting grain, fruit,
     or vegetables.
     </skos:definition>
        <rdfs:comment xml:lang="en">
     This excludes undistilled fermented beverages such as beer, wine, and
     cider.
     </rdfs:comment>
        <skos:prefLabel xml:lang="en">Distilled Beverage</skos:prefLabel>
        <skos:altLabel xml:lang="en">Liquor</skos:altLabel>
        <skos:altLabel xml:lang="en">Spirit</skos:altLabel>
        <status:term_status xml:lang="en">testing</status:term_status>
        <skos:prefLabel xml:lang="ko">증류주</skos:prefLabel>
        <rdfs:seeAlso rdf:resource="http://en.wikipedia.org/wiki/Distilled_beverage"/>
        <rdfs:isDefinedBy rdf:resource="http://rdfs.co/bevon/"/>
    </owl:Class>
    


    <!-- http://rdfs.co/bevon/DistilledSoju -->

    <owl:Class rdf:about="&bevon;DistilledSoju">
        <rdfs:subClassOf rdf:resource="&bevon;Soju"/>
        <dcterms:issued rdf:datatype="&xsd;date">2013-07-04+09:00</dcterms:issued>
        <dcterms:modified rdf:datatype="&xsd;date">2014-03-01+09:00</dcterms:modified>
        <skos:definition xml:lang="en">
    Distilled Soju is manufactured traditionally by distillation from grains,
    mainly rice.
    </skos:definition>
        <skos:prefLabel xml:lang="en">Distilled Soju</skos:prefLabel>
        <status:term_status xml:lang="en">unstable</status:term_status>
        <skos:prefLabel xml:lang="ko">증류식 소주</skos:prefLabel>
        <rdfs:isDefinedBy rdf:resource="http://rdfs.co/bevon/"/>
    </owl:Class>
    


    <!-- http://rdfs.co/bevon/Distillery -->

    <owl:Class rdf:about="&bevon;Distillery">
        <dcterms:issued rdf:datatype="&xsd;date">2013-07-04+09:00</dcterms:issued>
        <dcterms:modified rdf:datatype="&xsd;date">2014-03-01+09:00</dcterms:modified>
        <skos:definition xml:lang="en">
    A distillery is a premise where distillation takes place, especially
    distillation of alcohol.
    </skos:definition>
        <skos:prefLabel xml:lang="en">Distillery</skos:prefLabel>
        <status:term_status xml:lang="en">unstable</status:term_status>
        <skos:prefLabel xml:lang="ko">증류소</skos:prefLabel>
        <rdfs:seeAlso rdf:resource="http://en.wikipedia.org/wiki/Distillation"/>
        <rdfs:isDefinedBy rdf:resource="http://rdfs.co/bevon/"/>
    </owl:Class>
    


    <!-- http://rdfs.co/bevon/DryStout -->

    <owl:Class rdf:about="&bevon;DryStout">
        <rdfs:subClassOf rdf:resource="&bevon;Stout"/>
        <dcterms:issued rdf:datatype="&xsd;date">2013-07-04+09:00</dcterms:issued>
        <dcterms:modified rdf:datatype="&xsd;date">2014-03-01+09:00</dcterms:modified>
        <skos:definition xml:lang="en">
    Irish stout or dry stout is very dark or rich in color and it often has a
    &quot;roasted&quot; or coffee-like taste.
    </skos:definition>
        <skos:prefLabel xml:lang="en">Dry Stout</skos:prefLabel>
        <skos:altLabel xml:lang="en">Irish Stout</skos:altLabel>
        <status:term_status xml:lang="en">unstable</status:term_status>
        <skos:prefLabel xml:lang="ko">드라이 스타우트</skos:prefLabel>
        <rdfs:isDefinedBy rdf:resource="http://rdfs.co/bevon/"/>
    </owl:Class>
    


    <!-- http://rdfs.co/bevon/Dunkel -->

    <owl:Class rdf:about="&bevon;Dunkel">
        <rdfs:subClassOf rdf:resource="&bevon;Lager"/>
        <rdfs:subClassOf rdf:resource="&pto;Dunkel"/>
        <dcterms:issued rdf:datatype="&xsd;date">2013-07-04+09:00</dcterms:issued>
        <dcterms:modified rdf:datatype="&xsd;date">2014-03-01+09:00</dcterms:modified>
        <rdfs:comment xml:lang="en">
    Dunkel is the German word meaning dark, and dunkel beers typically range in
    color from amber to dark reddish brown. They are characterized by their
    smooth malty flavor.
    </rdfs:comment>
        <skos:definition xml:lang="en">
    Dunkel, or Dunkles, is a word used for several types of dark German lager.
    </skos:definition>
        <skos:prefLabel xml:lang="en">Dunkel</skos:prefLabel>
        <skos:altLabel xml:lang="en">Dunkles</skos:altLabel>
        <status:term_status xml:lang="en">unstable</status:term_status>
        <skos:prefLabel xml:lang="ko">둔켈</skos:prefLabel>
        <rdfs:isDefinedBy rdf:resource="http://rdfs.co/bevon/"/>
        <rdfs:seeAlso rdf:resource="https://en.wikipedia.org/wiki/Dunkel"/>
    </owl:Class>
    


