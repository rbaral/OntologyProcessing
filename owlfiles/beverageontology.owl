        <skos:prefLabel xml:lang="ko">광천수</skos:prefLabel>
        <rdfs:seeAlso rdf:resource="http://en.wikipedia.org/wiki/Mineral_water"/>
        <rdfs:isDefinedBy rdf:resource="http://rdfs.co/bevon/"/>
    </owl:Class>
    


